{
  "manifest_version": 2,
  "name": "Stadia Resolution",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["https://stadia.google.com/*"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "permissions": ["storage"]
}
